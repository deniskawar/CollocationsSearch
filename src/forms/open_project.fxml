<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="278.0" prefWidth="384.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.OpenProjectController">
   <children>
      <Button fx:id="openProjectButton" layoutX="113.0" layoutY="249.0" mnemonicParsing="false" onAction="#pressOpenProjectButton" prefHeight="19.0" prefWidth="70.0" text="Открыть" />
      <TableView fx:id="openProjectTable" prefHeight="243.0" prefWidth="414.0">
        <columns>
          <TableColumn fx:id="projectIdColumn" prefWidth="112.0" text="Номер проекта" />
          <TableColumn fx:id="projectNameColumn" prefWidth="301.0" text="Название проекта" />
        </columns>
      </TableView>
      <Button fx:id="deleteProjectButton" layoutX="227.0" layoutY="249.0" mnemonicParsing="false" onAction="#pressDeleteProjectButton" prefHeight="19.0" prefWidth="70.0" text="Удалить" />
   </children>
</AnchorPane>
